.search-button:hover {
    background-color: rgb(234, 146, 62);
    background-image: url('project://database/Assets/Packages/atomicmarketapiclient/Examples/UiToolkit/Assets/search.png?fileID=2800000&guid=815ec4dd775830942abcd2a73c903080&type=3#search');
