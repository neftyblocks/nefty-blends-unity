.select-button {
    width: auto;
    height: 45px;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
    margin-left: 3px;
    margin-right: 3px;
    margin-top: 21px;
    margin-bottom: 1px;
    padding-left: 18px;
    padding-right: 18px;
    padding-top: 20px;
    padding-bottom: 20px;
    background-color: rgb(36, 54, 137);
    border-top-left-radius: 12px;
    border-bottom-left-radius: 12px;
    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
    font-size: 18px;
    -unity-font-style: bold;
    color: rgb(255, 255, 255);
}

.select-button:hover {
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
    border-left-color: rgb(255, 254, 255);
    border-right-color: rgb(255, 254, 255);
    border-top-color: rgb(255, 254, 255);
    border-bottom-color: rgb(255, 254, 255);
    width: auto;
    height: 45px;
    flex-grow: 0;
    transition-duration: 0s;
    transition-timing-function: ease;
    transition-delay: 0s;
    color: rgb(255, 255, 255);
}

.search {
    width: 154px;
    height: 35px;
    border-top-left-radius: 18px;
    border-bottom-left-radius: 18px;
    border-top-right-radius: 18px;
    border-bottom-right-radius: 18px;
    border-left-color: rgba(255, 255, 255, 0);
    border-right-color: rgba(255, 255, 255, 0);
    border-top-color: rgba(255, 255, 255, 0);
    border-bottom-color: rgba(255, 255, 255, 0);
    background-color: rgba(88, 88, 88, 0.62);
    -unity-font-style: bold;
    position: absolute;
    bottom: auto;
    left: auto;
    top: 6px;
    right: 20px;
    -unity-text-align: middle-center;
}

.search:hover {
    background-color: rgb(96, 97, 101);
    scale: 1.05 1.05;
