.search-button {
    background-image: none;
    width: 26px;
    height: 26px;
    margin-left: 3px;
    margin-right: 3px;
    margin-top: 1px;
    margin-bottom: 1px;
    padding-left: 6px;
    padding-right: 6px;
    padding-top: 4px;
    padding-bottom: 4px;
    background-color: rgb(255, 255, 255);
}

.search-button:hover {
    background-color: rgb(234, 146, 62);
    background-image: url('project://database/Assets/Packages/hyperionapiclient/Examples/UiToolkit/Assets/search.png?fileID=2800000&guid=a3e0f7836e8aae44bb4eafb71ef3d549&type=3#search');
